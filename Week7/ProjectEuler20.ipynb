      "def factorial(i):\n",
      "    if i == 0:\n",
      "        return 1\n",
      "    else:\n",
      "        return i * factorial(i-1) \n",
      "def add_numbers(i):\n",
      "    Sum = 0\n",
      "    for n in str(i):\n",
      "        Sum += int(n)\n",
      "    return Sum\n",
